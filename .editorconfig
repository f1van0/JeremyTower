resharper_max_enum_members_on_line = 1
resharper_max_initializer_elements_on_line = 1
resharper_nested_ternary_style = simple_wrap
resharper_never_outdent_pipe_operators = false
resharper_new_line_before_while = true
resharper_outdent_binary_operators = false
resharper_place_accessor_attribute_on_same_line = false
resharper_place_field_attribute_on_same_line = if_owner_is_single_line
resharper_show_autodetect_configure_formatting_tip = false
resharper_space_between_attribute_sections = false
resharper_space_within_single_line_array_initializer_braces = false
resharper_use_continuous_indent_inside_parens = false
resharper_use_indent_from_vs = false
resharper_vb_alignment_tab_fill_style = use_tabs_only
resharper_vb_indent_style = tab
resharper_xmldoc_alignment_tab_fill_style = use_tabs_only
resharper_xmldoc_indent_style = tab

